{
    "name": "eliabrian/laravel-jikan",
    "description": "Laravel Package for Jikan API",
    "keywords": [
        "laravel",
        "jikan",
        "anime"
    ],
    "type": "library",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "7.9.x-dev"
    },
    "require-dev": {
        "orchestra/testbench": "dev-develop"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Eliabrian\\LaravelJikan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eliabrian\\LaravelJikan\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eliabrian\\LaravelJikan\\LaravelJikanServiceProvider"
            ],
            "aliases": {
                "Anime": "Eliabrian\\LaravelJikan\\Facades\\Anime",
                "Manga": "Eliabrian\\LaravelJikan\\Facades\\Manga"
            }
        }
    },
    "authors": [
        {
            "name": "Elia Brian Baskoro"
        }
    ],
    "minimum-stability": "dev",
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
